{
  "name": "changerawr/php",
  "description": "PHP SDK for interacting with self-hosted Changerawr instances",
  "type": "library",
  "license": "MIT",
  "version": "0.1.0",
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4|^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpstan/phpstan": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Changerawr\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Changerawr\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "format": "php-cs-fixer fix",
    "analyse": "phpstan analyse src --level=5"
  },
  "config": {
    "sort-packages": true
  }
}